{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@safekids-ai/common-plugin": ["libs/common-plugin/src/index.ts"],
      "@safekids-ai/extension-dev-utils": [
        "libs/extension-dev-utils/src/index.ts"
      ],
      "@safekids-ai/extension-hmr": ["libs/extension-hmr/src/index.ts"],
      "@safekids-ai/extension-i18n": ["libs/extension-i18n/src/index.ts"],
      "@safekids-ai/extension-shared": ["libs/extension-shared/src/index.ts"],
      "@safekids-ai/extension-storage": ["libs/extension-storage/src/index.ts"],
      "@safekids-ai/extension-vite-config": [
        "libs/extension-vite-config/src/index.ts"
      ],
      "@safekids-ai/ml-api-types": ["libs/ml-api-types/src/index.ts"],
      "@safekids-ai/onnx-common": ["libs/onnx-common/src/index.ts"],
      "@safekids-ai/onnx-web": ["libs/onnx-web/src/index.ts"],
      "@safekids-ai/onnx-node": ["libs/onnx-node/src/index.ts"],
      "@safekids-ai/onnx-model": ["libs/onnx-model/src/index.ts"],
      "@safekids-ai/nlp-js-common": ["libs/nlp-js-common/src/index.ts"],
      "@safekids-ai/nlp-js-react-native": [
        "libs/nlp-js-react-native/src/index.ts"
      ],
      "@safekids-ai/nlp-js-types": ["libs/nlp-js-types/src/index.ts"],
      "@safekids-ai/vision-js-common": ["libs/vision-js-common/src/index.ts"],
      "@safekids-ai/vision-js-node": ["libs/vision-js-node/src/index.ts"],
      "@safekids-ai/vision-js-react-native": [
        "libs/vision-js-web/src/index.ts"
      ],
      "@safekids-ai/vision-js-types": ["libs/vision-js-types/src/index.ts"],
      "@safekids-ai/vision-js-web": ["libs/vision-js-web/src/index.ts"],
      "@safekids-ai/ai-webcategory": ["libs/ai-webcategory/src/index.ts"],
      "@safekids-ai/web-category": ["libs/web-category/src/index.ts"],
      "@safekids-ai/web-category-types": [
        "libs/web-category-types/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
